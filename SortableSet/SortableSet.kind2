
Nat.to_U60 (n: Nat) : U60
Nat.to_U60  Nat.zero    = 0
Nat.to_U60 (Nat.succ x) = (+ 1 (Nat.to_U60 x))

Main : _
Main =
  let n = 10
  let set = SortableSet.fromList (List.map (List.range (U60.to_nat n)) (x => (Nat.to_U60 x)))

  (Pair.new
    (SortableSet.elements set)
    (SortableSet.sort set)
  )
