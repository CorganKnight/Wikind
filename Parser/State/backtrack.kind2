// Undoes the last n characters parsed
Parser.State.backtrack (n: U60) (state: Parser.State) : Parser.State
Parser.State.backtrack n (Parser.State.new left back index) =
  match Pair p = String.reverse_n_over back n left {
    new =>
      let left  = p.snd
      let back  = p.fst
      let index = U60.sub.saturating index n
      Parser.State.new left back index
  }