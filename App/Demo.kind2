Demo : App U60 {
  let init = 0
  let draw = (s: U60) =>
    let style = [
      (Pair.new "background" "#F0F0F0")
      (Pair.new "color" "blue")
    ]
    (App.DOM.node "div" [] style [
      if (U60.equal s 0) {
        (App.DOM.text "foo")
      } else {
        (App.DOM.text "bar")
      }
    ])
  let when = (event: App.Event) => (state: U60) => do IO {
    return Unit.new
  }
  App.new init draw when
}
