// Proof that (a / b)b + a % b = a for all (a, b)
Nat.mul_reverse (a: Nat) (b: Nat): (Equal
	(Nat.add (Nat.mul (Nat.div a b) b) (Nat.rem a b))
	a
)
Nat.mul_reverse Nat.zero  Nat.zero = Equal.refl
Nat.mul_reverse Nat.zero (Nat.succ b) = Equal.refl
Nat.mul_reverse (Nat.succ a) Nat.zero =
	let induction = a
	a