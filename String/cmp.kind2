String.cmp (a: String) (b: String) : Cmp
String.cmp  String.nil          String.nil         = Cmp.eql
String.cmp  String.nil         (String.cons bh bt) = Cmp.ltn
String.cmp (String.cons ah at)  String.nil         = Cmp.gtn
String.cmp (String.cons ah at) (String.cons bh bt) =
  let cmp = (Char.cmp ah bh)
  match Cmp cmp {
    ltn => Cmp.ltn
    eql => String.cmp at bt
    gtn => Cmp.gtn
  }